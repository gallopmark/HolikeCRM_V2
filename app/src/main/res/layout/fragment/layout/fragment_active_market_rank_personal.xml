<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_activity_dealers_rank_report">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="217dp"
        android:background="@drawable/high_light" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include layout="@layout/include_titlebar_transparent" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:overScrollMode="never"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:paddingTop="@dimen/dp_3">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_active_market_rank_personal_name"
                            android:layout_width="wrap_content"
                            android:layout_height="63dp"
                            android:gravity="center"
                            android:textColor="@color/color_while"
                            android:textSize="@dimen/textSize_30"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tv_active_market_rank_personal_city"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_15"
                            android:textColor="@color/textColor19"
                            android:textSize="@dimen/textSize_16"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tv_active_market_rank_personal_data"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_40"
                            android:textColor="@color/textColor19"
                            android:textSize="@dimen/textSize_14" />

                        <TextView
                            android:id="@+id/tv_active_market_rank_personal_score"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_16"
                            android:textColor="@color/color_while"
                            android:textSize="@dimen/textSize_16"
                            android:textStyle="bold" />
                    </LinearLayout>

                    <ImageView
                        android:layout_width="@dimen/dp_8"
                        android:layout_height="match_parent"
                        android:paddingBottom="@dimen/dp_4"
                        android:paddingTop="@dimen/dp_24"
                        android:scaleType="fitXY"
                        android:src="@drawable/distributor_division" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_active_market_rank_personal_rankPre"
                                android:layout_width="68dp"
                                android:layout_height="63dp"
                                android:background="@drawable/distributor_ranking"
                                android:gravity="center"
                                android:textColor="#FFFFFF00"
                                android:textSize="@dimen/textSize_20"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/tv_active_market_rank_personal_count"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/dp_9"
                                android:textColor="@color/color_while"
                                android:textSize="@dimen/textSize_14"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_15"
                            android:text="@string/report_ranking_national"
                            android:textColor="@color/textColor19"
                            android:textSize="@dimen/textSize_14" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_40"
                            android:text="@string/report_before_you"
                            android:textColor="@color/textColor19"
                            android:textSize="@dimen/textSize_14" />

                        <TextView
                            android:id="@+id/tv_active_market_rank_personal_pre_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_16"
                            android:textColor="@color/color_while"
                            android:textSize="@dimen/textSize_16"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>

                <View
                    style="@style/divider_horizontal"
                    android:background="#FF0066CC"
                    android:layout_marginTop="@dimen/dp_40"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginRight="@dimen/dp_12" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_32"
                    android:gravity="center|left"
                    android:orientation="horizontal">

                    <View
                        android:layout_width="@dimen/dp_3"
                        android:layout_height="@dimen/dp_16"
                        android:layout_marginLeft="@dimen/dp_12"
                        android:background="#FFFFDE02" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_5"
                        android:text="@string/report_top_three_national"
                        android:textColor="@color/color_while"
                        android:textSize="@dimen/dp_16"
                        android:textStyle="bold" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dp_40"
                    android:layout_marginTop="@dimen/dp_27"
                    android:gravity="center|top"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_32"
                        android:gravity="center|top"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dp_100"
                            android:layout_height="@dimen/dp_110"
                            android:src="@drawable/second" />

                        <TextView
                            android:id="@+id/tv_active_market_rank_personal_rank_no2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_14"
                            android:textColor="@color/color_while"
                            android:textSize="@dimen/textSize_14"
                            android:textStyle="bold" />

                        <View
                            android:layout_width="@dimen/dp_20"
                            android:layout_height="@dimen/dp_4"
                            android:layout_marginTop="@dimen/dp_7"
                            android:background="#FFA5BDD4" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_18"
                        android:gravity="center|top"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dp_100"
                            android:layout_height="@dimen/dp_110"
                            android:src="@drawable/first" />

                        <TextView
                            android:id="@+id/tv_active_market_rank_personal_rank_no1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_14"
                            android:textColor="@color/color_while"
                            android:textSize="@dimen/textSize_14"
                            android:textStyle="bold" />

                        <View
                            android:layout_width="@dimen/dp_20"
                            android:layout_height="@dimen/dp_4"
                            android:layout_marginTop="@dimen/dp_7"
                            android:background="#FFFFDE02" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_18"
                        android:layout_marginTop="@dimen/dp_32"
                        android:gravity="center|top"
                        android:orientation="vertical">

                        <ImageView
                            android:layout_width="@dimen/dp_100"
                            android:layout_height="@dimen/dp_110"
                            android:src="@drawable/third" />

                        <TextView
                            android:id="@+id/tv_active_market_rank_personal_rank_no3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_14"
                            android:textColor="@color/color_while"
                            android:textSize="@dimen/textSize_14"
                            android:textStyle="bold" />

                        <View
                            android:layout_width="@dimen/dp_20"
                            android:layout_height="@dimen/dp_4"
                            android:layout_marginTop="@dimen/dp_7"
                            android:background="#FFE8BB9E" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</FrameLayout>