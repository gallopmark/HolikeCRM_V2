<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_item_rv_terminal_check"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_40"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal">

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_terminal_check_area"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="59"
            android:gravity="center"
            android:text="@string/report_table_divide"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_terminal_check_name"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="51"
            android:gravity="center"
            android:text="@string/report_table_head"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_terminal_check_shop"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="45"
            android:gravity="center"
            android:text="@string/report_table_check_shop"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />


        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_terminal_check_reorganized"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="45"
            android:gravity="center"
            android:orientation="vertical"
            android:text="@string/report_table_reorganized"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12">

        </TextView>

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_terminal_check_unreorganize"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="45"
            android:gravity="center"
            android:text="@string/report_table_unreorganize"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_terminal_check_complete_percen"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="45"
            android:gravity="center"
            android:text="@string/report_table_complete_percen"
            android:textColor="@color/textColor12"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_terminal_check_rank"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="32"
            android:gravity="center"
            android:text="@string/report_table_rank"
            android:textColor="@color/textColor4"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>