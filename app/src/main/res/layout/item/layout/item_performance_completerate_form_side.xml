<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:minHeight="@dimen/dp_36"
        android:orientation="horizontal">

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_division"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_principal"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_target"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_55"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_complete"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_55"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_complete_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_55"
            android:textColor="@color/textColor30"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_year_on_year"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_ranking"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_36"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_today"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_36"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>
