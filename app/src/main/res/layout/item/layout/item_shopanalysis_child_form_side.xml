<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:minHeight="@dimen/dp_36"
        android:orientation="horizontal">

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_division"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_70"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_performance"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_effect"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_total_effect"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_sales_effect"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_payback"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_customer_price"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_shop_area"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_average_value"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_average_period"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_64"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_satisfaction"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_64"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scale_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_deposit_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_store_entry_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scale_turnover_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:visibility="invisible" />

        <View style="@style/divider_vertical" />
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>
