<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:minHeight="@dimen/dp_36"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_division"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_principal"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_stores"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_dealers"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_shop_area"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_average_value"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_performance"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:layout_gravity="center" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_effect"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_total_effect"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_sales_effect"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_customer_price"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_customers"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_preScale"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_scale"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_deposit"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_signatures"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>