<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_40"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal">

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_active_market_area_center"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="63"
            android:gravity="center"
            android:text="@string/report_table_center"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_active_market_area_active_market"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="61"
            android:gravity="center"
            android:text="@string/report_table_active_market"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_active_market_area_city"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="61"
            android:gravity="center"
            android:orientation="vertical"
            android:text="@string/report_table_serve_city"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12">

        </TextView>

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_active_market_area_data"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="84"
            android:gravity="center"
            android:text="@string/report_table_serve_date"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_active_market_area_days"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="34"
            android:gravity="center"
            android:text="@string/report_table_days"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_active_market_area_order_num"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="47"
            android:gravity="center"
            android:text="@string/report_table_order_num"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />
    </LinearLayout>


    <View style="@style/divider_horizontal" />
</LinearLayout>
