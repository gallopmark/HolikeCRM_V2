<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:minHeight="@dimen/dp_44"
        android:orientation="horizontal">

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_division"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_principal"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_new_customers"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_calls"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_scale"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_measure"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_drawing"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_deposit_customers"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_signatures"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_order_number"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_install"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_install_area"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_complete_rate"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_satisfaction"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_64"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_store_turnover_rate"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scale_rate"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_highSeas"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_received"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_contract_amount"
            style="@style/Scrollable_TextViewStyle2"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:textColor="@color/textColor8" />

        <View style="@style/divider_vertical" />
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>
