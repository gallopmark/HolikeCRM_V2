<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:minHeight="@dimen/dp_36"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_division"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_issued_count"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_effective_customers"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_invalid_customers"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_invalid_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scale_count"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scale_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:layout_gravity="center" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_earnest_count"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_earnest_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_earnest_money"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_sign_count"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_sign_rate"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_sign_money"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <LinearLayout
            android:id="@+id/ll_order_layout"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/tv_order_count"
                style="@style/FormTextStyle"
                android:layout_width="@dimen/dp_60"
                android:layout_height="match_parent" />

            <View style="@style/divider_vertical" />

            <TextView
                android:id="@+id/tv_order_rate"
                style="@style/FormTextStyle"
                android:layout_width="@dimen/dp_60"
                android:layout_height="match_parent" />

            <View style="@style/divider_vertical" />

            <TextView
                android:id="@+id/tv_order_money"
                style="@style/FormTextStyle"
                android:layout_width="@dimen/dp_60"
                android:layout_height="match_parent" />

            <View style="@style/divider_vertical" />
        </LinearLayout>
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>
