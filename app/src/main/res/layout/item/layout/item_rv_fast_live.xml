<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:id="@+id/ll_item_rv_order_report"
    android:layout_height="@dimen/dp_40"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal">

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_fast_live_area"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="63"
            android:gravity="center"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_fast_live_head"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="50"
            android:gravity="center"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_fast_live_total"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="50"
            android:gravity="center"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_fast_live_1"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="46"
            android:gravity="center"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_fast_live_2"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="46"
            android:gravity="center"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />


        <TextView
            android:id="@+id/tv_item_rv_fast_live_3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="46"
            android:gravity="center"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_item_rv_fast_live_4"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="46"
            android:gravity="center"
            android:textColor="@color/textColor8"
            android:textSize="@dimen/textSize_12" />

        <View style="@style/divider_vertical" />

    </LinearLayout>

    <View
        android:id="@+id/dv_item_rv_order_report_bottom"
        style="@style/divider_horizontal" />
</LinearLayout>