<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:minHeight="@dimen/dp_36"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_shop"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_guestrestaurant"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_hall"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_balcony"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_masterbedroom"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_guest_bedroom"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_childrenroom"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_elderroom"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_studyroom"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_multfunroom"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_kitchen"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_other"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_90"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:paddingLeft="@dimen/dp_4"
            android:paddingRight="@dimen/dp_4" />

        <View style="@style/divider_vertical" />
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>
