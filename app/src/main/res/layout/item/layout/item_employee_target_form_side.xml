<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_while"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:minHeight="@dimen/dp_36"
        android:orientation="horizontal">

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_name"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_80"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_newCountTarget"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_newCount"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_newCountPercent"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_prescaleCountTarget"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_prescaleCount"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_prescaleCountPercent"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_contractTotalTarget"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_contractTotal"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_contractTotalPercent"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_receiverTarget"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_receiver"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_receiverPercent"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scaleCountTarget"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scaleCount"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scaleCountPercent"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_picCountTarget"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_picCount"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_picCountTargetPercent"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_orderTotalTarget"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_orderTotal"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_orderTotalPercent"
            style="@style/FormTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent" />

        <View style="@style/divider_vertical" />
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>

