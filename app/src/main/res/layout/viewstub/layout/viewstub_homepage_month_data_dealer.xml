<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/dp_18"
    android:paddingLeft="@dimen/dp_12"
    android:paddingRight="@dimen/dp_12">


    <LinearLayout
        android:layout_width="152dp"
        android:layout_height="133dp"
        android:background="@drawable/six_angles"
        android:gravity="center"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="已收款（万）"
            android:textColor="@color/color_while"
            android:textSize="@dimen/textSize_10" />

        <TextView
            android:id="@+id/tv_homepage_month_data_date_dealer_collected"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="888.88"
            android:textColor="@color/color_while"
            android:textSize="@dimen/textSize_28"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_18"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center|left"
            android:orientation="horizontal">

            <View
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:background="@drawable/bg_round_orange" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_4"
                android:text="意向客户数"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />

            <TextView
                android:id="@+id/tv_homepage_month_data_date_dealer_intention"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_12"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="center|left"
            android:orientation="horizontal">

            <View
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:background="@drawable/bg_round_blue" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_4"
                android:text="量房数"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />

            <TextView
                android:id="@+id/tv_homepage_month_data_date_dealer_measure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_12"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="center|left"
            android:orientation="horizontal">

            <View
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:background="@drawable/bg_round_blue_shallow" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_4"
                android:text="出图数"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />

            <TextView
                android:id="@+id/tv_homepage_month_data_date_dealer_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_12"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="center|left"
            android:orientation="horizontal">

            <View
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:background="@drawable/bg_round_green" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_4"
                android:text="签约数"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />

            <TextView
                android:id="@+id/tv_homepage_month_data_date_dealer_signing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_12"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_homepage_month_data_date_dealer_install_customer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="center|left"
            android:orientation="horizontal">

            <View
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:background="@drawable/bg_round_purple" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_4"
                android:text="安装客户数"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />

            <TextView
                android:id="@+id/tv_homepage_month_data_date_dealer_install_customer"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_12"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_homepage_month_data_date_dealer_install_square"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="center|left"
            android:orientation="horizontal">

            <View
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_8"
                android:background="@drawable/bg_round_red" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_4"
                android:text="安装平方数"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />

            <TextView
                android:id="@+id/tv_homepage_month_data_date_dealer_install_square"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_12"
                android:textColor="@color/textColor8"
                android:textSize="@dimen/textSize_14" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>