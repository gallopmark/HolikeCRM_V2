<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_while"
    android:orientation="vertical">

    <View
        android:id="@+id/v_divider"
        style="@style/divider_horizontal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44"
        android:orientation="horizontal">

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_70"
            android:layout_height="match_parent"
            android:text="@string/division" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/report_table_performance" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/effect" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/total_human_effect" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/sales_effectiveness" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/payback" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:text="@string/customer_price" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_shop_area"
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/store_area" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/average_store_value" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_64"
            android:layout_height="match_parent"
            android:text="@string/average_turnover_period" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_64"
            android:layout_height="match_parent"
            android:text="@string/customer_satisfaction_tips2" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/scale_conversion_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/deposit_conversion_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/store_entry_turnover_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/scale_turnover_rate" />

        <View style="@style/divider_vertical" />

    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>
