<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_while"
    android:orientation="vertical">

    <View
        android:id="@+id/v_divider"
        style="@style/divider_horizontal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal">

        <TextView
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/principal" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_new_customers"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_new_customers" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_calls"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_calls" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_scale"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_reservation_scale" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_measure"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_measuring" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_drawing"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:text="@string/number_of_output_graphs" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_deposit_customers"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_deposit_customers" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_signatures"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_signatures" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_order_number"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_place_orders" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_install"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_install" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_store_turnover_rate"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/store_entry_turnover_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_scale_rate"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/scale_turnover_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_number_highSeas"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/number_of_customers_into_seas" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_received"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/payments_received" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_contract_amount"
            style="@style/Scrollable_TextViewStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/total_contract_amount" />

        <View style="@style/divider_vertical" />
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>
