<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_while"
    android:orientation="vertical">

    <View
        android:id="@+id/v_divider"
        style="@style/divider_horizontal" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal">

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_80"
            android:layout_height="match_parent"
            android:text="@string/employee" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/new_customer_target" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/number_of_new_customers" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/new_customer_completion_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/appointment_gauge_target" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/number_of_reservation_scale" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:text="@string/appointment_ruler_completion_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/transaction_target_million" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/transaction_million2" />

        <View style="@style/divider_vertical" />

        <TextView
            android:id="@+id/tv_order_number"
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/transaction_completion_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/payback_target_million" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_60"
            android:layout_height="match_parent"
            android:text="@string/payback_million" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/payback_completion_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/gauge_target" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/number_of_measuring" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/scale_completion_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/drawing_target" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/number_of_output_graphs" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/plot_completion_rate" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/order_target_million" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/order_million" />

        <View style="@style/divider_vertical" />

        <TextView
            style="@style/FirstRowTextStyle"
            android:layout_width="@dimen/dp_68"
            android:layout_height="match_parent"
            android:text="@string/order_completion_rate" />

        <View style="@style/divider_vertical" />
    </LinearLayout>

    <View style="@style/divider_horizontal" />
</LinearLayout>

