<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_quotation_info_finish_content_parent"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:minHeight="@dimen/dp_44"
    android:orientation="horizontal">

    <FrameLayout
        android:layout_width="@dimen/dp_140"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_size"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_size" />
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_48"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_color"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_color" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_48"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_unit"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_unit" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_56"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_benchmark"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_benchmark" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_56"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_retail_price"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"

                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_retail_price" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_56"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/tv_quotation_info_finish_coefficient_product"
                    style="@style/tv_item_base_customer_gray"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left|center"
                    android:maxLines="2"
                    android:paddingLeft="@dimen/dp_4"
                    android:paddingRight="@dimen/dp_4"
                    android:text="@string/quotation_info_total_coefficient_product" />

                <TextView
                    android:id="@+id/tv_quotation_info_product_name_fake"
                    style="@style/tv_item_base_customer_gray"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left|center"
                    android:maxLines="2"
                    android:paddingLeft="@dimen/dp_4"
                    android:paddingRight="@dimen/dp_4"
                    android:text="string/quotation_info_sizestring/quotation_info_sizestring/quotation_info_size"
                    android:visibility="invisible" />

            </FrameLayout>

        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_56"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_price_before"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_price_before" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_56"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_price_after"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_price_after" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_48"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_discount"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_discount" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_48"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_count"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_count" />
        </LinearLayout>

    </FrameLayout>

    <FrameLayout
        android:layout_width="@dimen/dp_48"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            <TextView
                android:id="@+id/tv_quotation_info_finish_total"
                style="@style/tv_item_base_customer_gray"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="left|center"
                android:maxLines="2"
                android:paddingLeft="@dimen/dp_4"
                android:paddingRight="@dimen/dp_4"
                android:text="@string/quotation_info_total_price" />
        </LinearLayout>

    </FrameLayout>

    <View
        android:id="@+id/v_footer"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_20"
        android:background="@color/textColor20"
        android:visibility="gone" />

</LinearLayout>